{
  "name": "dynamic-marquee-react",
  "version": "1.0.2",
  "description": "A `<Marquee />` component for React.",
  "main": "./dist/dynamic-marquee-react.js",
  "module": "./dist/dynamic-marquee-react.mjs",
  "types": "./dist/marquee.d.ts",
  "scripts": {
    "build": "rollup --config rollup.config.mjs",
    "watch": "rollup --config rollup.config.mjs --watch",
