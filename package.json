    "js",
    "javascript",
    "graphics",
    "casparcg"
  ],
  "author": "Tom Jenkinson <tjcode@outlook.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/tjenkinson/dynamic-marquee-react/issues"
  },
  "homepage": "https://github.com/tjenkinson/dynamic-marquee-react#readme",
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "15.2.1",
    "@rollup/plugin-typescript": "11.1.3",
    "@types/react": "18.2.21",
    "@types/react-dom": "18.2.7",
    "@typescript-eslint/eslint-plugin": "6.6.0",
    "@typescript-eslint/parser": "6.6.0",
    "dynamic-marquee": "2.6.3",
    "eslint": "8.49.0",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "husky": "8.0.3",
    "prettier": "3.0.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "rollup": "3.29.0",
    "tslib": "2.6.2",
    "typescript": "5.2.2"
  }
